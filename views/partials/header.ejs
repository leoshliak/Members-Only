<header>
    <div class="header-con">
    <div class="logo">
        <h1>Members Only</h1>
    </div>
    <nav id="nav">
        <ul>
            <li><a href="/">Home</a></li>
            <% if(locals.user) { %>
                <% if(locals.user.membership_status){ %>
                    <li><a href="/new-message">New Message</a></li>
                <!-- <li><a href="/messages">View Messages</a></li>-->
                  <li><a href="/profile/<%= user.id %>">View profile</a></li> 
                    <% } else { %>
                         <li><a href="/membership">Get Membership</a></li>
                         <li><a href="/profile/<%= user.id %>">View profile</a></li>
                        <% } %>
                <li><a href="/log-out">Log Out</a></li>
            <% } else { %>
                <li><a href="/log-in">Log In</a></li>
                <li><a href="/sign-up">Sign Up</a></li>
            <% } %>
        </ul>
    </nav>
</div>
</header>